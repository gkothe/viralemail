	<queryString>
		<![CDATA[SELECT desc_prod,
       pedido_item.val_unit,
       qtd_prod,
       pedido_item.val_unit * qtd_prod          AS val_subtotalprod,
       pedido.id_pedido,
       COALESCE(bairros.desc_bairro, '-')       AS desc_bairro,
       pedido.flag_status,
       data_pedido,
       val_totalprod,
       val_entrega,
       data_pedido_resposta,
       num_ped,
       pedido.flag_modopagamento,
       flag_pedido_ret_entre,
       perc_pagamento,
       nome_pessoa,
       desc_endereco_entrega,
       desc_endereco_num_entrega,
       desc_endereco_complemento_entrega,
       num_telefonecontato_cliente,
       data_cancelamento,
       desc_obs,
       desc_motivo,
       ( perc_pagamento * val_totalprod ) / 100 AS tragoaqui_perc,
       pedido_item.val_unit as summary_total,
'' as desc_status
FROM   pedido
       INNER JOIN distribuidora
               ON distribuidora.id_distribuidora = pedido.id_distribuidora
       LEFT JOIN bairros
              ON bairros.cod_bairro = pedido.cod_bairro
       INNER JOIN pedido_item
               ON pedido.id_pedido = pedido_item.id_pedido
       INNER JOIN produtos_distribuidora
               ON produtos_distribuidora.id_prod = pedido_item.id_prod
                  AND produtos_distribuidora.id_distribuidora = 1
       INNER JOIN produtos
               ON produtos.id_prod = produtos_distribuidora.id_prod
       LEFT JOIN pedido_motivo_cancelamento
              ON pedido_motivo_cancelamento.id_pedido = pedido.id_pedido
       LEFT JOIN motivos_cancelamento
              ON pedido_motivo_cancelamento.cod_motivo =
                 motivos_cancelamento.cod_motivo]]>
	</queryString>
